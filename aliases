alias ls="ls -G"
alias ll="ls -la"
alias la="ls -a"
alias rm="rm -r"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias o="open ."
alias vi="vim"
alias reload="exec $SHELL -l"
alias pcat="pygmentize -g"
alias dunnet="emacs -batch -l dunnet"
alias pyserver="python -m SimpleHTTPServer"
